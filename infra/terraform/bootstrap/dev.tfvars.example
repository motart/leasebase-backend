# Development Environment Bootstrap Configuration
# Copy to dev.tfvars and apply with: terraform apply -var-file=dev.tfvars

environment        = "dev"
aws_region         = "us-east-1"
vpc_cidr           = "10.10.0.0/16"
availability_zones = ["us-east-1a", "us-east-1b"]

# NAT Gateway disabled for dev (cost savings)
# ECS tasks will run in public subnets with public IPs
enable_nat_gateway = false

# GitHub repository for OIDC authentication
github_repo = "motart/leasebase-backend"
